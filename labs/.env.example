# Synesthetic Labs Environment Configuration
# Copy this file to .env and fill in the required values

# API Keys
GEMINI_API_KEY=your_api_key_here  # Required for external generation

# Model Configuration
GEMINI_MODEL=gemini-2.0-flash  # Default model for external generation

# Feature Flags
LABS_EXTERNAL_LIVE=false  # Set to 'true' to enable external API calls
LABS_FAIL_FAST=false      # Set to 'true' to abort on MCP validation failures

# Schema Version
LABS_SCHEMA_VERSION=0.7.3  # Asset schema version (0.7.3 or 0.7.4)

# Output Path Configuration
# LABS_OUTPUT_DIR=custom/path  # Optional: Override default output directory